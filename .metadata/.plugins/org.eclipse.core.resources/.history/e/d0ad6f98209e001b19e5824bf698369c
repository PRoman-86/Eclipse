package HW11;

public class StaffUtils {
	
	public static String searchByName(Employee[] employeeArr, String searchName) {
		for (int i=0; i<employeeArr.length; i++) {
			if (employeeArr[i].name.equals(searchName))
				return employeeArr[i].name;
		}
		
		return "Not Found";
	}
	
